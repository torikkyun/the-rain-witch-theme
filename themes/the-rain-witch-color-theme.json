	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#4a5d6a"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#b4c8d4"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"entity.name.constant"
			],
			"settings": {
				"foreground": "#9f854a"
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity",
				"entity.name"
			],
			"settings": {
				"foreground": "#6b9fc8"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#c44a48"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#5285b8"
			}
		},
		{
			"name": "Operators",
