	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546e7a"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#e8f4f8"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"entity.name.constant"
			],
			"settings": {
				"foreground": "#b8964a"
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity",
				"entity.name"
			],
			"settings": {
				"foreground": "#90caf9"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#f44336"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#4a90e2"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"punctuation"
			],
			"settings": {
				"foreground": "#8bb6d9"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#81c784"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#d4a574"
			}
		},
		{
			"name": "Built-in constant",
			"scope": [
