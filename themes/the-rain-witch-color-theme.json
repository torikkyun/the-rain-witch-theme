	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6a7f8f"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#d4dae3"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant",
				"entity.name.constant"
			],
			"settings": {
				"foreground": "#c5a068"
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity",
				"entity.name"
			],
			"settings": {
				"foreground": "#7ab0e0"
			}
		},
		{
			"name": "Invalid",
